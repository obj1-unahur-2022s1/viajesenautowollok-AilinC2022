import remiseras.*

//Roxana:

test "Cobro de Roxana a su clienta Ludmila por 10 kms" {
	roxana.precioPorViaje(ludmila, 10)
	assert.that(roxana.precioPorViaje(ludmila, 10) == 180)
}


test "Cobro de Roxana a su clienta AnaMaria por 10 kms" {
	roxana.precioPorViaje(anaMaria, 10)
	assert.that(roxana.precioPorViaje(anaMaria, 10) == 300)
}

test "Cobro de Roxana a su clienta Teresa por 10 kms" {
	roxana.precioPorViaje(teresa, 10)
	assert.that(roxana.precioPorViaje(teresa, 10) == 220)
}

