import remiseras.*

//Roxana:

test "Cobro de Roxana a su clienta Ludmila por 2 kms" {
	roxana.precioPorViaje(ludmila, 2)
	assert.that(roxana.precioPorViaje(ludmila, 2) == 36)
}


test "Cobro de Roxana a su clienta AnaMaria por 2 kms" {
	roxana.precioPorViaje(anaMaria, 2)
	assert.that(roxana.precioPorViaje(anaMaria, 2) == 60)
}

test "Cobro de Roxana a su clienta Teresa por 2 kms" {
	roxana.precioPorViaje(teresa, 2)
	assert.that(roxana.precioPorViaje(teresa, 2) == 44)
}

